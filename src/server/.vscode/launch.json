{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Nest.js: debug server-side",
      "type": "node-terminal",
      "request": "launch",
      "command": "yarn nest start --watch --debug"
    },
    {
      "name": "Nest.js: debug browser-side",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000"
    }
  ],
  "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"]
}
