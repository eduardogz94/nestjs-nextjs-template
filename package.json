{
  "name": "next-nest-template",
  "version": "0.1.0",
  "private": true,
  "description": "template for a next-nest SSR application",
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "nest": "nest ",
    "prebuild": "rimraf dist ",
    "build": "NODE_ENV=production nest build && cd src/client && yarn build",
    "format": "prettier --write \"src/**/*.{ts, tsx}\" ",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/server/main",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx, mjs, cjs}\"",
    "test": "jest --setupFiles dotenv-flow/config --collect-coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:request": "jest --setupFiles dotenv-flow/config --config ./test/jest-request.json",
    "console": "ts-node -r tsconfig-paths/register src/server/console.ts",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "database:init": "ts-node -r tsconfig-paths/register src/server/console/seed.initdb.ts",
    "clean-modules": "npx rimraf node_modules && npx rimraf package-lock.json && yarn cache clean --force && yarn install && cd src/client && yarn clean-modules",
    "prepare": "husky install"
  },
  "author": "Eduardo Gonzalez Zangarini",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.3.3",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "nestjs-console": "^8.0.0",
    "next": "^13.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "pgtools": "^0.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "type-fest": "^3.5.4",
    "typeorm": "^0.3.11",
    "url": "^0.11.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@next/eslint-plugin-next": "^13.1.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/prettier": "^2.7.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.3.0",
    "class-transformer": "^0.5.1",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.10",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "prettier": "^2.8.1",
    "rimraf": "^4.1.2",
    "standard": "^17.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.5"
  }
}
